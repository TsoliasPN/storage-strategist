{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Storage Strategist",
  "version": "0.1.0",
  "identifier": "com.storage.strategist",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Storage Strategist",
        "width": 1280,
        "height": 820,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": false
  }
}
